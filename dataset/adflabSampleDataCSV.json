{
	"name": "adflabSampleDataCSV",
	"properties": {
		"linkedServiceName": {
			"referenceName": "adflabsourcestorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string",
				"defaultValue": "csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "@dataset().filename",
				"folderPath": "inputfiles",
				"container": "rawdata"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "EmployeeId",
				"type": "String"
			},
			{
				"name": "EmpName",
				"type": "String"
			},
			{
				"name": "Age",
				"type": "String"
			},
			{
				"name": "Location",
				"type": "String"
			},
			{
				"name": "Role",
				"type": "String"
			}
		]
	}
}